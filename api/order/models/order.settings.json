{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "paid": {
      "type": "boolean",
      "default": false
    },
    "quantity": {
      "type": "biginteger"
    },
    "price": {
      "type": "biginteger"
    },
    "finish": {
      "type": "string"
    },
    "size": {
      "type": "string"
    },
    "payment": {
      "model": "payment",
      "via": "orders"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "product": {
      "model": "product",
      "via": "orders"
    },
    "buy_order": {
      "type": "string"
    }
  }
}
